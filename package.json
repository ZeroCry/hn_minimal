  },
  "homepage": "https://github.com/developit/hn_minimal-preact",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^3.2.2",
